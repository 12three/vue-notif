{
  "name": "vue-notify-plugin",
  "description": "Non-blocking notifications for Vue.js",
  "version": "0.1.2",
  "private": false,
  "main": "./dist/vueNotifyPlugin.common.js",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "web",
    "front-end",
    "vue",
    "vuejs",
    "notification",
    "vue-notification",
    "vue-notifications"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/12three/vue-notify.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build --target lib --name vueNotifyPlugin ./src/plugin/NotificationList.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "css-loader": "^1.0.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 8"
  ]
}
